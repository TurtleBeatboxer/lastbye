<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="8:84f2eb6e7a4f506004e5c6dea780b72b" author="Michas">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="Profile" columnName="public_profile_link" />
                <columnExists tableName="Profile" columnName="membership_level" />
                <columnExists tableName="Profile" columnName="finished_editing" />
                <columnExists tableName="Profile" columnName="edits_left" />
            </not>
        </preConditions>
        <addColumn tableName="profile">
            <column name="membership_level" type="number">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="public_profile_link" type="varchar(256)">
                <constraints nullable="true" unique="true"/>
            </column>
            <column name="finished_editing" type="boolean">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="edits_left" type="number">
                <constraints nullable="true" unique="false"/>
            </column>
        </addColumn>



    </changeSet>
</databaseChangeLog>
